<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>Commom.NoContracts}"
		title="{i18n>Commom.SelectAContract}"
		search="handleSearchContract"
		confirm="handleCloseContractSelection"
		close="handleCloseContractSelection"
		items="{
			path: '/ContractCollection'
		}" >
		<StandardListItem
			title="{Description}"
			description="{path:'DocTotal', type:'sap.ui.model.type.Currency'}"			
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>
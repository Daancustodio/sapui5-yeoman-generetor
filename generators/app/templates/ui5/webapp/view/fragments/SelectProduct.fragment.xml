<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>Commom.NoProducts}"
		title="{i18n>Commom.SelectProducts}"
		search="handleSearch"
		confirm="handleClose"
		close="handleClose"
		items="{
			path: '/ProductCollection'
		}" >
		<StandardListItem
			title="{ItemName}"
			description="{ItemCode}"
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>